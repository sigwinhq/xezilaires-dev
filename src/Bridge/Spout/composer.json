{
    "name": "sigwin/xezilaires-spout",
    "type": "library",
    "description": "Use Xezilaires together with Spout to iterate structured Excel spreadsheets, normalize rows into value objects, validate, serialize into CSV, JSON, XML",
    "license": "MIT",
    "authors": [
        {
            "name": "Dalibor KarloviÄ‡",
            "email": "dalibor.karlovic@sigwin.hr"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "box/spout": "^3.0",
        "sigwin/xezilaires": "^0.4"
    },
    "require-dev": {
        "doctrine/annotations": "^1.11",
        "nyholm/nsa": "^1.1",
        "phpunit/phpunit": "^9.5",
        "symfony/property-access": "^4.4 || ^5.0",
        "symfony/property-info": "^4.4 || ^5.0"
    },
    "suggest": {
        "sigwin/xezilaires-symfony": "Symfony bundle integration, bin/xezilaires use"
    },
    "config": {
        "preferred-install": {
            "sigwin/xezilaires": "source"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Xezilaires\\Bridge\\Spout\\": "."
        },
        "exclude-from-classmap": [
            "/Test/"
        ]
    },
    "minimum-stability": "dev"
}
