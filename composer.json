{
    "name": "dkarlovi/xezilaires",
    "type": "library",
    "description": "Iterate Excel spreadsheets, normalize rows into value objects, validate, serialize into CSV, JSON, XML",
    "license": "MIT",
    "authors": [
        {
            "name": "Dalibor KarloviÄ‡",
            "email": "dalibor.karlovic@flexolabs.io"
        }
    ],
    "require": {
        "php": ">= 7.2.0",
        "ext-json": "*",
        "symfony/options-resolver": "^3.4 || ^4.0",
        "symfony/serializer": "^3.4 || ^4.0"
    },
    "conflict": {
        "box/spout": "< 3.0",
        "phpoffice/phpspreadsheet": "< 1.4.1"
    },
    "require-dev": {
        "box/spout": "^3.0",
        "doctrine/annotations": "^1.6",
        "matthiasnoback/symfony-dependency-injection-test": "^3.1",
        "nicmart/tree": "^0.2,>= 0.2.4",
        "nyholm/nsa": "^1.1",
        "nyholm/symfony-bundle-test": "^1.4",
        "phpbench/phpbench": "^0.16",
        "phpoffice/phpspreadsheet": "^1.4,>= 1.4.1",
        "phpunit/phpunit": "^7.0",
        "symfony/console": "^3.4 || ^4.0",
        "symfony/framework-bundle": "^3.4 || ^4.0",
        "symfony/property-access": "^3.4 || ^4.0",
        "symfony/property-info": "^3.4 || ^4.0"
    },
    "suggest": {
        "box/spout": "Read Excel files with Spout",
        "phpoffice/phpspreadsheet": "Read Excel files with PhpSpreadsheet",
        "symfony/console": "bin/xezilaires use",
        "symfony/framework-bundle": "Symfony bundle integration, bin/xezilaires use"
    },
    "config": {
        "platform": {
            "ext-gd": "1.3.1"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Xezilaires\\": "src/Xezilaires"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xezilaires\\Test\\": "tests/Xezilaires"
        }
    },
    "bin": [
        "bin/xezilaires"
    ]
}
