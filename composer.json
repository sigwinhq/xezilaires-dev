{
    "name": "dkarlovi/xezilaires",
    "description": "Iterate Excel spreadsheets, normalize rows into value objects, validate, serialize into CSV, JSON, XML",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Dalibor KarloviÄ‡",
            "email": "dalibor.karlovic@flexolabs.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "Xezilaires\\": "src/Xezilaires"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xezilaires\\Test\\": "tests/Xezilaires"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "ext-gd": "1.3.1"
        }
    },
    "require": {
        "php": ">= 7.1.3",
        "phpoffice/phpspreadsheet": "^1.0",
        "symfony/options-resolver": "^3.0 | ^4.0",
        "symfony/serializer": "^3.0 | ^4.0"
    },
    "require-dev": {
        "doctrine/annotations": "^1.0",
        "nicmart/tree": "^0.2, >= 0.2.4",
        "phpunit/phpunit": "^6.0 | ^7.0",
        "symfony/console": "^3.0 | ^4.0",
        "symfony/property-access": "^3.0 | ^4.0",
        "symfony/property-info": "^3.0 | ^4.0"
    }
}
